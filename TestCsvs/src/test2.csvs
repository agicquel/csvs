load csv "./deniro.csv";

index = 0;

while(index < count(col csv("Year"))){
	print("index = ");
	print(index);
	print("    ");
	
	if(cell csv(index, "Year") >= 2000) {
		cell csv(index, "Year") = cell csv(index, "Year") - 2000;
	}
	else {
		cell csv(index, "Year") = cell csv(index, "Year") - 1900;
	}
	index = index + 1;
}

store csv "./deniro2.csv";