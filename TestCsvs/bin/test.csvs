load variableName "./path/to/file.csv"; // load a csv
store variableName "./path/to/file.csv"; // save csv
create variableName; // create an empty csv
export variableName; // export csv to json

// declare and initialize a variable
set variableName "value";
set variableName variable2;
set variableName 123;
set variableName 2 < 4;
set (cell variableName (123, 456)) 789;

// print values to stdout
print "value";
print variableName;
print 123;
print 2 < 4;

// selectors
print (row variableName (123));
print (col variableName (123));
print (cell variableName (123, 456));
print (field variableName (123));

// delete
delete (row variableName (123));
delete (col variableName (123));

set i 10;
while(i < 10) {
	set i (i+1);
}
if(i == 10) {
	print "ten!";
}
else {
	print "not ten.";
}